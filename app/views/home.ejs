<%include partials/header%>
    <nav class="navbar navbar-fixed-top nav-white" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Engineering Assistant</a>
            </div>
            <div class=" collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li><a href="/login">Login</a></li>
                    <li><a href="/signup">Signup</a></li>
                    <li>
                        <form action="/logout" method="POST">
                            <input class="btn btn-transparent" type="submit" value="Logout" />
                        </form>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="jumbotron"></div>
    <div class="container">
        <div class="input-group">
            <input type="text" class="form-control" placeholder="Search for...">
            <span class="input-group-btn">
        <button class="btn btn-default" type="button">Go!</button>
      </span>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-lg-2 col-md-2 col-sm-0"></div>
            <div class="col-lg-8 col-md-8 col-sm-12">
                <ul class="list-group">
                    <% data.forEach(function(topics) { %>
                        <a href="/topics/<%=topics[" topic_name "]%>">
                            <li class="card list-group-item main-topic">
                                <%= topics["topic_name"]%>
                            </li>
                        </a>
                        <% })%>
                </ul>
            </div>
            <div class="col-lg-2 col-md-2 col-sm-0"></div>
        </div>
    </div>
    <%include partials/footer%>
